{
	"name": "dawdawd-test",
	"version": "1.0.11",
	"description": "SDK for Yellow Change",
	"author": {
		"name": "0x32 ðŸ§Š",
		"url": "https://github.com/0xx32"
	},
	"type": "module",
	"main": " dist/index.js",
	"module": " dist/index.js",
	"types": " dist/index.d.ts",
	"scripts": {
		"start": "tsx ./src/index.ts",
		"dev": "tsx watch ./src/index.ts",
		"format": "prettier . --write",
		"prepare": "husky",
		"build:types": "tsc -p tsconfig.json",
		"build:rollup24": "rollup -c rollup.config.js",
		"build:rollup": "rollup -c rollup.config.js",
		"build": "shx rm -rf dist && pnpm build:types && pnpm build:swc",
		"copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
		"build:swc": "swc index.ts -d dist --extensions .ts --copy-files",
		"build:roll": "shx rm -rf dist && pnpm build:rollup"
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md",
		"package.json"
	],
	"license": "ISC",
	"devDependencies": {
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@swc/cli": "^0.4.0",
		"@swc/core": "^1.7.22",
		"@types/node": "^22.5.1",
		"copyfiles": "^2.4.1",
		"esbuild": "^0.23.1",
		"husky": "^9.1.5",
		"lint-staged": "^15.2.9",
		"prettier": "^3.3.3",
		"rollup": "^4.21.2",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-includepaths": "^0.2.4",
		"rollup-plugin-size-snapshot": "^0.12.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript-paths": "^1.5.0",
		"rollup-plugin-typescript2": "^0.36.0",
		"shx": "^0.3.4",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"tsx": "^4.19.0",
		"turbo": "^2.1.1",
		"typescript": "^5.5.4"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"axios": "^1.7.5"
	},
	"homepage": "https://github.com/0xx32/yellow-changer-sdk",
