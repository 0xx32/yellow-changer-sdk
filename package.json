{
	"name": "yellow-changer-sdk",
	"version": "0.0.2",
	"description": "SDK for Yellow Change",
	"author": {
		"name": "0x32 ðŸ§Š",
		"url": "https://github.com/0xx32"
	},
	"type": "module",
	"scripts": {
		"start": "tsx example.ts",
		"dev": "tsx watch example.ts",
		"format": "prettier . --write",
		"prepare": "husky",
		"build:types": "tsc --emitDeclarationOnly",
		"build:rollup": "rollup -c rollup.config.js",
		"build": "shx rm -rf dist && pnpm build:types  && pnpm build:swc",
		"build:swc": "swc src index.ts -d dist --extensions .ts --copy-files",
		"build:roll": "shx rm -rf dist && pnpm build:rollup"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md",
		"package.json"
	],
	"license": "ISC",
	"devDependencies": {
		"@swc/cli": "^0.4.0",
		"@swc/core": "^1.7.22",
		"@types/node": "^22.5.1",
		"esbuild": "^0.23.1",
		"husky": "^9.1.5",
		"lint-staged": "^15.2.9",
		"pkgroll": "^2.4.2",
		"prettier": "^3.3.3",
		"rollup": "^4.21.2",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-typescript-paths": "^1.5.0",
		"shx": "^0.3.4",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"tsx": "^4.19.0",
		"typescript": "^5.5.4"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"axios": "^1.7.5"
	},
	"homepage": "https://github.com/0xx32/yellow-changer-sdk",
	"repository": {
		"type": "git",
		"url": "https://github.com/0xx32/yellow-changer-sdk.git"
	},
	"keywords": [
		"yellow",
		"change",
		"api",
		"sdk",
		"crypto"
	]
}
